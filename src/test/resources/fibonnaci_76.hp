fibonacci :: (number: s64): s64 =
    number == 0 or number == 1
        ? number
        : fibonacci(number - 1) + fibonacci(number - 2)

main :: s64 = foo()

foo :: s64 {
    var answer: s64 = 0
    for (var i: s64 = 10; i > 0; i--) {
        if i == 9 continue else if i == 7 break
        answer = answer + fibonacci(i)
    }
    answer
}