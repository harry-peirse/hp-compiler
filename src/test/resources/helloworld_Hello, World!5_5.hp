external void putchar(char c)

void print(char[] string) {
    int i = -1;
    char c = string[++i];
    while(c != 0) {
        putchar(c);
        c = string[++i];
    }
}

void println(char[] string) {
    print(string);
    putchar('\n');
}

int main() {
    print("Hello, ");
    println("World!");
    print("5");

    return 5;
}